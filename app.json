{
  "expo": {
    "name": "niflux-locate",
    "slug": "niflux-locate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/Niflux-Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access to your camera",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access to your camera",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyC4shVfCWvRSP6joaPdgQbwfmrATPPPqcw"
      },
      "bundleIdentifier": "com.rounakexpo.nifluxlocate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/Niflux-Logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA"
      ],
      "package": "com.rounakexpo.nifluxlocate",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDKxmdg4iLrRiHCrj__WVc38Zf53eUuamE"
        }
      }
    },
    "web": {
      "favicon": "./assets/Images/Niflux-Logo.png"
    }
  }
}
